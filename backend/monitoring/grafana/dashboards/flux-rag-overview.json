{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(fluxrag_http_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{ endpoint }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "Error Rate (%)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(fluxrag_http_requests_total{status=~\"5..\"}[5m])) / sum(rate(fluxrag_http_requests_total[5m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "thresholds": { "steps": [{ "value": 0, "color": "green" }, { "value": 1, "color": "yellow" }, { "value": 5, "color": "red" }] } }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "fluxrag_active_connections",
          "legendFormat": "Active",
          "refId": "A"
        }
      ]
    },
    {
      "title": "HTTP Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(fluxrag_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(fluxrag_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(fluxrag_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "LLM Provider Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(fluxrag_llm_request_duration_seconds_bucket[5m])) by (le, provider))",
          "legendFormat": "{{ provider }} p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(fluxrag_cache_hits_total[5m])) / (sum(rate(fluxrag_cache_hits_total[5m])) + sum(rate(fluxrag_cache_misses_total[5m]))) * 100",
          "legendFormat": "Hit Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "min": 0, "max": 100, "thresholds": { "steps": [{ "value": 0, "color": "red" }, { "value": 50, "color": "yellow" }, { "value": 80, "color": "green" }] } }
      }
    },
    {
      "title": "RAG Retrieval Latency (p95)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(fluxrag_rag_retrieval_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Document Count",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
      "targets": [
        {
          "expr": "fluxrag_document_count",
          "legendFormat": "Documents",
          "refId": "A"
        }
      ]
    },
    {
      "title": "LLM Tokens (input vs output)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "sum(rate(fluxrag_llm_tokens_total{direction=\"input\"}[5m]))",
          "legendFormat": "Input tokens/s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(fluxrag_llm_tokens_total{direction=\"output\"}[5m]))",
          "legendFormat": "Output tokens/s",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" }
      }
    },
    {
      "title": "RAG Relevance Score Distribution",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "sum(rate(fluxrag_rag_relevance_score_bucket[5m])) by (le)",
          "legendFormat": "{{ le }}",
          "refId": "A",
          "format": "heatmap"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["flux-rag"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "flux-rag Overview",
  "uid": "flux-rag-overview",
  "version": 1
}
